{
  "name": "broccoli",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.0",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/react-use-size": "^2.1.0",
    "@chakra-ui/styled-system": "^2.9.1",
    "@dagrejs/dagre": "^1.0.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/react": "^0.24.8",
    "@tanstack/match-sorter-utils": "^8.7.6",
    "@tanstack/react-query": "^5.17.10",
    "@tanstack/react-table": "^8.7.9",
    "dayjs": "^1.11.5",
    "file-saver": "^2.0.5",
    "formik": "^2.4.2",
    "framer-motion": "^10.13.1",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "numeral": "^2.0.6",
    "oidc-client-ts": "^2.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-query-kit": "^3.1.0",
    "react-router-dom": "^6.8.2",
    "react-select": "^5.7.0",
    "reactflow": "^11.8.3",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.10",
    "zustand": "^4.3.8"
  },
  "scripts": {
    "start": "run-p watch:css start:vite",
    "start:vite": "vite --mode cognito",
    "build": "npm run compile && npm run build:css && npm run build:vite",
    "build:vite": "vite build",
    "serve": "vite preview",
    "analyze": "source-map-explorer 'build/assets/*.js'",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mjs,cjs}\"",
    "lint": "eslint ./src --ext js,jsx,mjs,cjs,ts,tsx --fix",
    "test": "",
    "compile": "tsc --noEmit",
    "coverage": "vitest --coverage",
    "e2e": "cypress open",
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/assets/styles/app.css -o src/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/assets/styles/app.css -o src/index.css --watch",
    "bundle-report": "cross-env npx vite-bundle-visualizer",
    "codegen": "openapi-rq -i ../backend/build/classes/java/main/META-INF/swagger/broccoli-1.0.yml -o ./src/gens/openapi"
  },
  "babelMacros": {
    "twin": {
      "debug": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^0.5.2",
    "@faker-js/faker": "^8.0.2",
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@tanstack/react-query-devtools": "^5.17.10",
    "@types/d3": "^7.4.3",
    "@types/d3-force": "^3.0.9",
    "@types/file-saver": "^2.0.5",
    "@types/lodash-es": "^4.17.8",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.15.1",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^9.1.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-twin": "^1.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "cypress": "~12.13.0",
    "delay-cli": "^1.1.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "mathjs": "^11.9.1",
    "msw": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "~7.0.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.3",
    "twin.macro": "^3.3.1",
    "typescript": "^4.9.5",
    "vite": "^5.0.11",
    "vite-plugin-babel-macros": "^1.0.6",
    "vitest": "^0.29.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
